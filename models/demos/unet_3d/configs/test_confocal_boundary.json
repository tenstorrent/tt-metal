{
    "model_path": "./models/demos/unet_3d/data/models/confocal_boundary.safetensors",
    "backend": "ttnn",
    "model": {
        "in_channels": 1,
        "out_channels": 1,
        "base_channels": 16,
        "num_levels": 3,
        "num_groups": 8,
        "scale_factor": 2
    },
    "predictor": {
        "name": "StandardPredictor",
        "performance_metric": [ "mean_iou", "dice" ],
        "label_internal_path": "label",
        "raw_internal_path": "raw"
    },
    "dataset": {
        "raw_internal_path": "raw",
        "label_internal_path": "label",
        "type": "StandardHDF5Dataset",
        "output_dir": "./models/demos/unet_3d/data/predictions/confocal_boundary/validation",
        "batch_size_per_device": 1,
        "num_workers": 4,
        "global_normalization": true,
          "file_paths": [
              "./models/demos/unet_3d/data/datasets/confocal_boundary/validation/N_420_ds2x.h5"
          ],
          "slice_builder": {
              "name": "SliceBuilder",
              "patch_shape": [32, 32, 64],
              "stride_shape": [32, 32, 64],
              "halo_shape": [0, 0, 0]
          },
          "transformer": {
              "raw": [
                  {
                      "name": "Standardize"
                  },
                  {
                      "name": "ToTensor",
                      "expand_dims": true
                  }
              ],
              "label": [
                  {
                      "name": "StandardLabelToBoundary",
                      "append_label": false
                  }
              ]
          }
    }
}
