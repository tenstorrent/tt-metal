# --- Meshes ---------------------------------------------------------------

mesh_descriptors {
  name: "M0"
  arch: WORMHOLE_B0
  device_topology { dims: [ 2, 4 ] }
  host_topology   { dims: [ 1, 1 ] }
  channels {
    count: 2
    policy: STRICT
  }
}

# --- Graphs ---------------------------------------------------------------

graph_descriptors {
  name: "G0"
  type: "FABRIC"
  # Instances: mesh ids 0..1 (all 2x4)
  instances { mesh { mesh_descriptor: "M0" mesh_id: 0 } }
  instances { mesh { mesh_descriptor: "M0" mesh_id: 1 } }

  # Strict connections between specific devices
  # M0 D1 <-> M1 D1
  connections {
    nodes { mesh { mesh_descriptor: "M0" mesh_id: 0 device_id: 1 } }
    nodes { mesh { mesh_descriptor: "M0" mesh_id: 1 device_id: 1 } }
    channels { count: 1 }
  }
  # M0 D2 <-> M1 D2
  connections {
    nodes { mesh { mesh_descriptor: "M0" mesh_id: 0 device_id: 2 } }
    nodes { mesh { mesh_descriptor: "M0" mesh_id: 1 device_id: 2 } }
    channels { count: 1 }
  }
  # M0 D5 <-> M1 D5
  connections {
    nodes { mesh { mesh_descriptor: "M0" mesh_id: 0 device_id: 5 } }
    nodes { mesh { mesh_descriptor: "M0" mesh_id: 1 device_id: 5 } }
    channels { count: 1 }
  }
  # M0 D6 <-> M1 D6
  connections {
    nodes { mesh { mesh_descriptor: "M0" mesh_id: 0 device_id: 6 } }
    nodes { mesh { mesh_descriptor: "M0" mesh_id: 1 device_id: 6 } }
    channels { count: 1 }
  }
}

# --- Instantiation ----------------------------------------------------------
top_level_instance { graph { graph_descriptor: "G0" graph_id: 0 } }
