# --- Meshes ---------------------------------------------------------------

mesh_descriptors {
  name: "M0"
  arch: WORMHOLE_B0
  device_topology { dims: [ 8, 2 ] }
  host_topology   { dims: [ 1, 1 ] }
  channels {
    count: 2
    policy: RELAXED
  }
}

# --- Graphs ---------------------------------------------------------------

graph_descriptors {
  name: "G0"
  type: "FABRIC"
  # Instances: mesh ids 0..1 (all 8x2)
  instances { mesh { mesh_descriptor: "M0" mesh_id: 0 } }
  instances { mesh { mesh_descriptor: "M0" mesh_id: 1 } }

  # Strict connections between specific devices
  # For 8x2 mesh: connect rightmost column of M0 to leftmost column of M1
  # M0 D6 (row 0, col 6) <-> M1 D0 (row 0, col 0)
  connections {
    nodes { mesh { mesh_descriptor: "M0" mesh_id: 0 device_id: 1 } }
    nodes { mesh { mesh_descriptor: "M0" mesh_id: 1 device_id: 0 } }
    channels { count: 2 }
  }
  connections {
    nodes { mesh { mesh_descriptor: "M0" mesh_id: 0 device_id: 13 } }
    nodes { mesh { mesh_descriptor: "M0" mesh_id: 1 device_id: 14 } }
    channels { count: 2 }
  }
}

# --- Instantiation ----------------------------------------------------------
top_level_instance { graph { graph_descriptor: "G0" graph_id: 0 } }

# --- Pinnings ---------------------------------------------------------------

pinnings {
  logical_fabric_node_id { mesh_id: 0 chip_id: 0 }
  physical_asic_position { tray_id: 1 asic_location: 1 }
}
pinnings {
  logical_fabric_node_id { mesh_id: 0 chip_id: 15 }
  physical_asic_position { tray_id: 3 asic_location: 5 }
}
pinnings {
  logical_fabric_node_id { mesh_id: 1 chip_id: 0 }
  physical_asic_position { tray_id: 2 asic_location: 5 }
}
pinnings {
  logical_fabric_node_id { mesh_id: 1 chip_id: 15 }
  physical_asic_position { tray_id: 4 asic_location: 1 }
}
