# --- Meshes ---------------------------------------------------------------

mesh_descriptors {
  name: "M0"
  arch: WORMHOLE_B0
  device_topology { dims: [ 2, 8 ] }
  host_topology   { dims: [ 1, 1 ] }
  channels {
    count: 4
    policy: RELAXED
  }
}

# --- Graphs ---------------------------------------------------------------

graph_descriptors {
  name: "G0"
  type: "FABRIC"
  # Instances: mesh ids 0..1 (all 8x2)
  instances { mesh { mesh_descriptor: "M0" mesh_id: 0 } }
  instances { mesh { mesh_descriptor: "M0" mesh_id: 1 } }

  connections {
    nodes { mesh { mesh_descriptor: "M0" mesh_id: 0 device_id: 9} }
    nodes { mesh { mesh_descriptor: "M0" mesh_id: 1 device_id: 9} }
    channels { count: 1 policy: RELAXED }
  }

  connections {
    nodes { mesh { mesh_descriptor: "M0" mesh_id: 0 device_id: 8} }
    nodes { mesh { mesh_descriptor: "M0" mesh_id: 1 device_id: 8} }
    channels { count: 1 policy: RELAXED }
  }

}
# --- Instantiation ----------------------------------------------------------
top_level_instance { graph { graph_descriptor: "G0" graph_id: 0 } }
