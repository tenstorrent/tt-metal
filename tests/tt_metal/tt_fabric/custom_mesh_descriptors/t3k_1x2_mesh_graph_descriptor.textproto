# --- Meshes ---------------------------------------------------------------

mesh_descriptors {
  name: "M0"
  arch: WORMHOLE_B0
  device_topology { dims: [ 1, 2 ] }
  host_topology   { dims: [ 1, 1 ] }
  channels {
    count: 2
    policy: STRICT
  }
}

# --- Graphs ---------------------------------------------------------------

graph_descriptors {
  name: "G0"
  type: "FABRIC"
  # Instances: mesh ids 0..3 (all 1x2)
  instances { mesh { mesh_descriptor: "M0" mesh_id: 0 } }
  instances { mesh { mesh_descriptor: "M0" mesh_id: 1 } }
  instances { mesh { mesh_descriptor: "M0" mesh_id: 2 } }
  instances { mesh { mesh_descriptor: "M0" mesh_id: 3 } }

  # Horizontal between 0-1
  connections {
    nodes { mesh { mesh_descriptor: "M0" mesh_id: 0 device_id: 1 } }
    nodes { mesh { mesh_descriptor: "M0" mesh_id: 1 device_id: 0 } }
    channels { count: 2 }
    routing_direction : [ E, W ]
  }
  connections {
    nodes { mesh { mesh_descriptor: "M0" mesh_id: 2 device_id: 1 } }
    nodes { mesh { mesh_descriptor: "M0" mesh_id: 3 device_id: 0 } }
    channels { count: 2 }
    routing_direction : [ E, W ]
  }

  # Vertical 0-2 (S,N) twice for ports 0,1
  connections {
    nodes { mesh { mesh_descriptor: "M0" mesh_id: 0 device_id: 0 } }
    nodes { mesh { mesh_descriptor: "M0" mesh_id: 2 device_id: 0 } }
    channels { count: 2 }
    routing_direction : [ S, N ]
  }
  connections {
    nodes { mesh { mesh_descriptor: "M0" mesh_id: 0 device_id: 1 } }
    nodes { mesh { mesh_descriptor: "M0" mesh_id: 2 device_id: 1 } }
    channels { count: 2 }
    routing_direction : [ S, N ]
  }
  connections {
    nodes { mesh { mesh_descriptor: "M0" mesh_id: 1 device_id: 0 } }
    nodes { mesh { mesh_descriptor: "M0" mesh_id: 3 device_id: 0 } }
    channels { count: 2 }
    routing_direction : [ S, N ]
  }
  connections {
    nodes { mesh { mesh_descriptor: "M0" mesh_id: 1 device_id: 1 } }
    nodes { mesh { mesh_descriptor: "M0" mesh_id: 3 device_id: 1 } }
    channels { count: 2 }
    routing_direction : [ S, N ]
  }
}

# --- Instantiation ----------------------------------------------------------
top_level_instance { graph { graph_descriptor: "G0" graph_id: 0 } }
