# Example: Complex Multi-Level Mixing
# This example shows groupings containing multiple types of lower-level groupings

base_units {
  # Base units are predefined constants - ASIC_LOCATION_1 through ASIC_LOCATION_8
}

groupings {
  name: "trays"
  items: [
    { asic_location: ASIC_LOCATION_1 },
    { asic_location: ASIC_LOCATION_2 },
    { asic_location: ASIC_LOCATION_3 },
    { asic_location: ASIC_LOCATION_4 },
    { asic_location: ASIC_LOCATION_5 },
    { asic_location: ASIC_LOCATION_6 },
    { asic_location: ASIC_LOCATION_7 },
    { asic_location: ASIC_LOCATION_8 }
  ]
}

groupings {
  name: "hosts"
  items: [
    { grouping_ref { grouping_name: "trays" count: 4 } }
  ]
}

groupings {
  name: "meshes"
  items: [
    { grouping_ref { grouping_name: "hosts" count: 1 } }  # Each mesh = 1 host (meshes can have 1 item)
  ]
}

groupings {
  name: "pods"
  items: [
    { grouping_ref { grouping_name: "meshes" count: 2 } }  # Each pod = 2 meshes
  ]
}

groupings {
  name: "superpods"
  items: [
    { grouping_ref { grouping_name: "pods" count: 2 } },  # Each superpod = 2 pods
    { grouping_ref { grouping_name: "meshes" count: 2 } }  # Also contains 2 meshes directly (in addition to pods)
  ]
}

groupings {
  name: "clusters"
  items: [
    { grouping_ref { grouping_name: "superpods" count: 2 } },
    { grouping_ref { grouping_name: "pods" count: 2 } },  # Also contains 2 pods directly
    { grouping_ref { grouping_name: "meshes" count: 3 } }  # Also contains 3 meshes directly
  ]
}
