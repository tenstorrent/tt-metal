# Descriptor with only 2 hosts instead of 4
# This should trigger a host count mismatch warning when merged with base_intrapod.textproto
graph_templates {
  key: "small_cluster"
  value {
    children {
      name: "node1"
      node_ref { node_descriptor: "BH_GALAXY" }
    }
    children {
      name: "node2"
      node_ref { node_descriptor: "BH_GALAXY" }
    }
    internal_connections {
      key: "QSFP_DD"
      value {
        connections {
          port_a { path: ["node1"] tray_id: 0 port_id: 0 }
          port_b { path: ["node2"] tray_id: 0 port_id: 0 }
        }
      }
    }
  }
}

root_instance {
  template_name: "small_cluster"
  child_mappings {
    key: "node1"
    value { host_id: 0 }
  }
  child_mappings {
    key: "node2"
    value { host_id: 1 }
  }
}
